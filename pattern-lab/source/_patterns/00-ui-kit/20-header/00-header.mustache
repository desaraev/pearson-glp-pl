{{#header}}
	<header>
		<div class="logo"></div>
		<div class="actions">
			{{#buttons}}
				<span class="{{className}}">{{#notification}} {{> ui-kit-single-icon}} {{/notification}} {{> ui-kit-icon }}</span>
			{{/buttons}}
			<div class="username">
				{{#avatar}}<span class="avatar"><img src="../../images/{{image}}" alt="avatar"></span>{{/avatar}}
				<span class="name">{{name}}</span>
				<span class="nameicon">{{> ui-kit-single-icon}} </span>
			</div>
		</div>
	</header>
{{/header}}
