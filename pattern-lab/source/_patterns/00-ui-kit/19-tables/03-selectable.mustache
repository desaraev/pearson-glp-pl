{{#selecTable}}
	<table class="selectable">
		<thead>
		<tr>
			<th width="50" scope="col">{{> ui-kit-checkboxes }}</th>
			{{#th}}
				<th scope="col">{{label}}</th>
			{{/th}}
		</tr>
		</thead>
		<tbody>
		{{#tr}}
			<tr{{#selected}} class="selected"{{/selected}}>
				<td width="50" scope="row">{{> ui-kit-checkboxes }}</td>
				{{#td}}
					<td>{{label}}</td>
				{{/td}}
			</tr>
		{{/tr}}
		</tbody>
	</table>
{{/selecTable}}