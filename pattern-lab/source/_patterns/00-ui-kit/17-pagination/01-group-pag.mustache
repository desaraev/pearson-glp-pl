{{#paginationGroup}}
<div class="pagination group">
    <div class="control left">
        {{#controlLeft}} {{> ui-kit-icon }} {{/controlLeft}}
    </div>
    <div class="group pe-label">
        <span class="first">1</span> - <span class="last">5</span> of <span class="total">{{total}}</span>
    </div>
    <div class="control right">
        {{#controlRight}} {{> ui-kit-icon }} {{/controlRight}}
    </div>
</div>
{{/paginationGroup}}