{{#checkBox}}
	<div class="pe-checkbox">
		<input type="checkbox" aria-label="{{ariaLabel}}" id="{{id}}" value="{{label}}"{{#disabled}} disabled{{/disabled}}{{#checked}} checked{{/checked}}/>
		<label for="{{id}}">{{label}}</label>
		<span>
			{{> ui-kit-single-icon }}
      </span>
	</div>
{{/checkBox}}